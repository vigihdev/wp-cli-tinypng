{
    "name": "vigihdev/wp-cli-tinypng",
    "description": "Command khusus untuk mengelola TinyPNG dengan sintaks yang lebih ringkas dari WP-CLI standar",
    "homepage": "https://github.com/vigihdev/wp-cli-tinypng",
    "keywords": [
        "wordpress",
        "wp-cli",
        "cli",
        "tinypng",
        "commands",
        "development"
    ],
    "type": "wp-cli-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Vigih Dev",
            "email": "your-email@example.com",
            "homepage": "https://github.com/vigihdev",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.id"
        }
    ],
    "require": {
        "php": "^8.1",
        "vigihdev/wp-cli-models": "dev-main",
        "tinify/tinify": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "10.5.x-dev",
        "symfony/dotenv": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Vigihdev\\WpCliTinypng\\": "src/"
        },
        "files": [
            "tinify-command.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Vigihdev\\WpCliTinypng\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0
    },
    "scripts": {
        "test": "phpunit",
        "build": "console git:push && console satis:build && cd $HOME/.wp-cli/packages && console composer:update vigihdev/wp-cli-tinypng && cd $HOME/WordpressCli/wp-cli-tinypng",
        "dev": "php -S localhost:1472 dev.php"
    }
}
